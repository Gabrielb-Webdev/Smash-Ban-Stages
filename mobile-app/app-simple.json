{
  "expo": {
    "name": "AFK Smash",
    "slug": "afk-smash-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#DC2626",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.afksmash.mobile"
    },
    "android": {
      "package": "com.afksmash.mobile",
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "afk-smash"
  }
}